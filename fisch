-- Teleportes (simples exemplo)
Tab2:AddDropdown({
    Name = "Teleportar Para",
    Description = "Selecione um local",
    Options = {"Spawn", "Loja", "Dock"},
    Callback = function(value)
        if value == "Spawn" then
            game.Players.LocalPlayer.Character:PivotTo(CFrame.new(0, 10, 0))
        elseif value == "Loja" then
            game.Players.LocalPlayer.Character:PivotTo(CFrame.new(100, 10, 200))
        elseif value == "Dock" then
            game.Players.LocalPlayer.Character:PivotTo(CFrame.new(-50, 10, 300))
        end
    end
})

-- Config (FPS etc.)
Tab3:AddToggle({
    Name = "No Fog",
    Description = "Remove neblina para mais FPS",
    Default = false,
    Callback = function(v)
        if v then
            game.Lighting.FogEnd = 1e6
        else
            game.Lighting.FogEnd = 1000
        end
    end
})

Tab3:AddToggle({
    Name = "Fullbright",
    Description = "IluminaÃ§Ã£o total",
    Default = false,
    Callback = function(v)
        if v then
            game.Lighting.Brightness = 5
            game.Lighting.ClockTime = 12
            game.Lighting.GlobalShadows = false
            game.Lighting.Ambient = Color3.new(1, 1, 1)
        else
            game.Lighting.Brightness = 2
            game.Lighting.ClockTime = 14
            game.Lighting.GlobalShadows = true
            game.Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
        end
    end
})
